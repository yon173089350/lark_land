<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox stylesheets="@../../css/styles.css" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="landlark.winboot.controller.manaul.TransferFileController">
    <padding>
        <Insets bottom="10" left="10" right="10" top="10" />
    </padding>
    <HBox BorderPane.alignment="CENTER">
        <padding>
            <Insets bottom="10" left="10" right="10" top="10" />
        </padding>
        <Button onAction="#onUploadFileClick" text="Upload" />
    </HBox>


    <TableView fx:id="manualTableView" VBox.vgrow="ALWAYS">
        <columns>
            <TableColumn fx:id="manualTableColumnMessage" text="Message" />
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
        </columnResizePolicy>
    </TableView>

    <HBox BorderPane.alignment="CENTER">
        <padding>
            <Insets bottom="10" left="10" right="10" top="10" />
        </padding>
        <Button text="Submit" />
    </HBox>
    <cursor>
        <Cursor fx:constant="DEFAULT" />
    </cursor>

</VBox>
