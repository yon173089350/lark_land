<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="simple" targetRuntime="MyBatis3Simple">
		<commentGenerator>
			<property name="suppressDate" value="true" />
		</commentGenerator>
		<jdbcConnection
			connectionURL="jdbc:postgresql://localhost:5432/lark"
			driverClass="org.postgresql.Driver" password="lark" userId="lark" />
		<javaModelGenerator targetPackage="land.lark.model"
			targetProject="lark/src/main/java" />
		<sqlMapGenerator targetPackage="land.lark.mapper"
			targetProject="lark/src/main/resources" />
		<javaClientGenerator
			targetPackage="land.lark.mapper" targetProject="lark/src/main/java"
			type="XMLMAPPER" />
		<table tableName="usr" />
	</context>
</generatorConfiguration>